<OutStreams>
  <Plot name="plotROM2" overwrite="False">
    <filename>y2ROM</filename>
    <plotSettings>
      <plot>
        <type>scatter</type>
        <x>sampledData|Input|x1</x>
        <y>sampledData|Input|x2</y>
        <z>sampledData|Input|x3</z>
        <colorMap>sampledData|Output|y2</colorMap>
        <linewidths>0</linewidths>
        <cmap>coolwarm</cmap>
      </plot>
    </plotSettings>
    <actions>
      <how>screen</how>
      <title>
        <text>Sampled Input Space: ROM2</text>
      </title>
    </actions>
  </Plot>
  <Plot name="plotTraining2" overwrite="False">
