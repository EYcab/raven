  <OutStreams>
    <Print name='dump'>
      <type>csv</type>
      <source>data</source>
    </Print>
    <Print name='centroid'>
      <type>csv</type>
      <source>clusterInfo</source>
    </Print>
    <Plot  name="inputData" overwrite="True" >
      <filename>inputData</filename>
      <plotSettings>
        <plot>
          <type>line</type>
          <x>data|Output|time</x>
          <y>data|Output|x</y>
          <z>data|Output|y</z>
          <interpPointsX>300</interpPointsX>
          <xlabel>time</xlabel>
          <ylabel>x</ylabel>
          <zlabel>y</zlabel>
        </plot>
      </plotSettings>
      <actions>
        <how>screen</how>
      </actions>
    </Plot>
    <Plot  name="clusteredData" overwrite="True" >
      <filename>clusteredData</filename>
      <plotSettings>
        <plot>
          <type>line</type>
          <x>data|Output|time</x>
          <y>data|Output|x</y>
          <z>data|Output|y</z>
          <colorMap>data|Output|labels</colorMap>
          <interpPointsX>300</interpPointsX>
          <xlabel>time</xlabel>
          <ylabel>x</ylabel>
          <zlabel>y</zlabel>
        </plot>
      </plotSettings>
      <actions>
        <how>screen</how>
        <colorbar>off</colorbar>
      </actions>
    </Plot>
    <Plot  name="clusterCenters" overwrite="True" >
      <filename>clusterCenters</filename>
      <plotSettings>
        <plot>
          <type>line</type>
          <x>clusterInfo|Output|time</x>
          <y>clusterInfo|Output|x</y>
          <z>clusterInfo|Output|y</z>
          <colorMap>clusterInfo|Input|labels</colorMap>
          <interpPointsX>300</interpPointsX>
          <xlabel>time</xlabel>
          <ylabel>x</ylabel>
          <zlabel>y</zlabel>
        </plot>
      </plotSettings>
      <actions>
        <how>screen</how>
        <colorbar>off</colorbar>
      </actions>
    </Plot>
  </OutStreams>